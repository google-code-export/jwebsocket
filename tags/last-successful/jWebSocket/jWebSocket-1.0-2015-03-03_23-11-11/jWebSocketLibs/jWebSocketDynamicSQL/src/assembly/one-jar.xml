<?xml version="1.0" encoding="UTF-8"?>
<assembly>
	<!-- jWebSocketDynamicSQL -->
	<!-- this id is appended to filename given in pom.xml -->
	<id>${project.version}</id>
	<formats>
		<format>jar</format>
	</formats>
	
	<includeBaseDirectory>false</includeBaseDirectory>
	
	<dependencySets>
		
		<dependencySet>
			<outputDirectory/>
			<includes>
				<include>${groupId}:${artifactId}</include>
				<include>org.apache.ddlutils:ddlutils</include>
				<include>com.healthmarketscience.common:common-util</include>
				<include>com.healthmarketscience.sqlbuilder:sqlbuilder</include>
			</includes>
			<unpack>true</unpack>
		</dependencySet>
		
	</dependencySets>
	
</assembly>