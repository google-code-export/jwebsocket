
<Gauge2 coordWidth="1000" coordHeight="1000" left="0" top="0" bottom="0" right="0">

    <!-- Label background -->
    <Gauge2RectangularBorder centerX="590" centerY="455" width="230" height="70" stroke="#FBFBFB" strokeWidth="12" arcSize="30">
        <Gauge2RectangularBorder.Filler>
            <Gauge2LinearGradientFiller color1="#000000" color2="#000000" angle="0"/>
        </Gauge2RectangularBorder.Filler>
    </Gauge2RectangularBorder>
    <Gauge2RectangularBorder centerX="590" centerY="650" width="230" height="70" stroke="#FBFBFB" strokeWidth="12" arcSize="30">
        <Gauge2RectangularBorder.Filler>
            <Gauge2LinearGradientFiller color1="#000000" color2="#000000" angle="0"/>
        </Gauge2RectangularBorder.Filler>
    </Gauge2RectangularBorder>

    <!-- Labels -->
    <Gauge2Label id="label1" text="Memory" font="arial 46 bold" foreColor="#367CBB" y="450" x="380"/>
    <Gauge2Label id="label2" text="0" font="40 bold" foreColor="#FBFBFB" y="455" x="590" anchorHorizontal="center" anchorVertical="center"/>
    <Gauge2Label id="label3" text="Swap" font="arial 46 bold" foreColor="#70BF7B" y="643" x="370"/>
    <Gauge2Label id="label4" text="0" font="40 bold" foreColor="#FBFBFB" y="650" x="590" anchorHorizontal="center" anchorVertical="center"/>

    <!-- Range -->
    <Gauge2RadialRange centerX="500" centerY="500" width="1000" height="1000" startAngle="-90" endAngle="90">
        <!-- Scale -->
        <Gauge2RadialScale startValue="0" endValue="100">
            <Gauge2RadialScaleSection sectionWidth="70" color="#FBFBFB" radius="360" startValue="-1" endValue="101"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#367CBB" radius="360" startValue="1" endValue="19"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#367CBB" radius="360" startValue="21" endValue="39"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#367CBB" radius="360" startValue="41" endValue="59"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#C5A05A" radius="360" startValue="61" endValue="79"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#CD6766" radius="360" startValue="81" endValue="99"/>
        </Gauge2RadialScale>
        
        <Gauge2RadialScale font="46 bold" foreColor="#000000" radius="430" startValue="0" endValue="100" labelCount="6">
            
            <!-- Scale Selection -->
            <Gauge2RadialScaleSection sectionWidth="40" color="#367CBB" stroke="#FBFBFB" strokeWidth="2" radius="360" startValue="1" endValue="59"/>
           

            <!--Needle-->
            <Gauge2RadialNeedle id="needle1" value="0" stroke="#FBFBFB" strokeWidth="10"
                                innerRadius="240" innerWidth="70"
                                outerRadius="305" outerWidth="1">
                <Gauge2RadialNeedle.Filler>
                    <Gauge2RadialGradientFiller color1="#367CBB" color2="#367CBB"/>
                </Gauge2RadialNeedle.Filler>
            </Gauge2RadialNeedle>
        </Gauge2RadialScale>

        <!-- Ticks -->
        <Gauge2RadialTicks tickWidth="20" tickLength="60" color="#FBFBFB" tickCount="6" radius="360"/>
        
    </Gauge2RadialRange>

    <!-- Range -->
    <Gauge2RadialRange centerX="500" centerY="500" width="1000" height="1000" startAngle="110" endAngle="250">
        
        <!-- Scale -->
        <Gauge2RadialScale startValue="0" endValue="100">
            <Gauge2RadialScaleSection sectionWidth="70" color="#FBFBFB" radius="360" startValue="-1" endValue="101"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#CD6766" radius="360" startValue="1" endValue="19"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#C5A05A" radius="360" startValue="21" endValue="39"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#70BF7B" radius="360" startValue="41" endValue="59"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#70BF7B" radius="360" startValue="61" endValue="79"/>
            <Gauge2RadialScaleSection sectionWidth="40" color="#70BF7B" radius="360" startValue="81" endValue="99"/>
        </Gauge2RadialScale>
        
        <Gauge2RadialScale radius="430" startValue="0" endValue="100">
            
            <!-- Scale Selection -->
            <Gauge2RadialScaleSection sectionWidth="40" color="#70BF7B" stroke="#FBFBFB" strokeWidth="2" radius="360" startValue="59" endValue="99"/>
        </Gauge2RadialScale>

        <!-- Ticks -->
        <Gauge2RadialTicks tickWidth="20" tickLength="60" color="#FBFBFB" tickCount="6" radius="360"/>
        
    </Gauge2RadialRange>

    <!-- Range -->
    <Gauge2RadialRange centerX="500" centerY="500" width="1000" height="1000" startAngle="-110" endAngle="-250">
        <Gauge2RadialScale font="46 bold" foreColor="#000000" radius="430" startValue="0" endValue="100" labelCount="6">

	<!-- Needle -->
        <Gauge2RadialNeedle id="needle2" value="0" stroke="#FBFBFB" strokeWidth="10"
                                innerRadius="240" innerWidth="70"
                                outerRadius="305" outerWidth="1">
                <Gauge2RadialNeedle.Filler>
                    <Gauge2RadialGradientFiller color1="#70BF7B" color2="#70BF7B"/>
                </Gauge2RadialNeedle.Filler>
            </Gauge2RadialNeedle>
        </Gauge2RadialScale>

    </Gauge2RadialRange>

</Gauge2>

        
